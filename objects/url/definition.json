{
  "name": "url",
  "meta-category": "network",
  "description": "url object describes an url along with its normalized field (like extracted using faup parsing library) and its metadata.",
  "version": 1,
  "attributes": {
    "url": {
      "misp-attribute": "url",
      "misp-usage-frequency": 1
    },
    "last-seen": {
      "misp-attribute": "datetime",
      "misp-usage-frequency": 0
    },
    "text": {
      "misp-attribute": "text",
      "misp-usage-frequency": 0
    },
    "credential": {
      "misp-attribute": "text",
      "misp-usage-frequency": 0
    },
    "subdomain": {
      "misp-attribute": "text",
      "misp-usage-frequency": 0
    },
    "domain": {
      "misp-attribute": "domain",
      "misp-usage-frequency": 0
    },
    "domain_without_tld": {
      "misp-attribute": "text",
      "misp-usage-frequency": 0
    },
    "host": {
      "misp-attribute": "hostname",
      "misp-usage-frequency": 0
    },
    "tld": {
      "misp-attribute": "tld",
      "misp-usage-frequency": 0
    },
    "port": {
      "misp-attribute": "text",
      "misp-usage-frequency": 0
    },
    "scheme": {
      "misp-attribute": "text",
      "misp-usage-frequency": 0,
      "sane_default": [
        "http",
        "https",
        "ftp",
        "gopher",
        "sip"
      ]
    },
    "first-seen": {
      "misp-attribute": "datetime",
      "misp-usage-frequency": 0
    },
    "resource_path": {
      "misp-attribute": "text",
      "misp-usage-frequency": 0
    },
    "query_string": {
      "misp-attribute": "text",
      "misp-usage-frequency": 0
    },
    "fragment": {
      "misp-attribute": "text",
      "misp-usage-frequency": 0
    }
  },
  "requiredOneOf": [
    "url"
  ]
}
